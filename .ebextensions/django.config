container_commands:
  01_migrate:
    command: "source venv/bin/activate && python manage.py migrate --noinput"
    leader_only: true
  02_collectstatic:
    command: "source venv/bin/activate && python manage.py collectstatic --noinput"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: trashr/wsgi.py