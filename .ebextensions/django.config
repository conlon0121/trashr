packages:
  yum:
    git: []
    postgresql96-devel: []
container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && cd /opt/python/current/app && python manage.py migrate --noinput"
    leader_only: true
  01_collectstatic:
    command: "source /opt/python/run/venv/bin/activate && cd /opt/python/current/app && python manage.py collectstatic"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: trashr/wsgi.py